{
  "version": "1",
  "cloud": {
    "project": "01HAQMMECEYQYKFJN8MP16QC41"
  },
  "packages": [
    {
      "name": "sqliteDao",
      "schema": "assets/migrations",
      "queries": "assets/queries",
      "path": "internal/database/sqliteDao",
      "engine": "sqlite",
      "database": {
        "uri": "file:ondeck?mode=memory&cache=shared"
      },
      "rules": [
        "sqlc/db-prepare"
      ],
      "emit_json_tags": true,
      "emit_db_tags": true,
      "emit_exported_queries": true,
      "emit_prepared_queries": true,
      "emit_interface": true,
      "emit_pointers_for_null_types": true,
      "json_tags_case_style": true,
      "emit_enum_valid_method": true,
      "emit_all_enum_values": true,
      "output_copyfrom_file_name": "copyfrom.go",
      "overrides": [
        {
          "db_type": "int",
          "go_type": {
            "type": "int"
          }
        }
      ]
    }
  ]
}
